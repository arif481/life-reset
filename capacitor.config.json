{
  "appId": "com.lifereset.app",
  "appName": "Life Reset",
  "webDir": "www",
  "server": {
    "androidScheme": "https",
    "cleartext": false
  },
  "android": {
    "allowMixedContent": false,
    "captureInput": true,
    "webContentsDebuggingEnabled": false,
    "appendUserAgent": "LifeResetApp"
  },
  "plugins": {
    "SplashScreen": {
      "launchShowDuration": 2000,
      "launchAutoHide": true,
      "backgroundColor": "#4361ee",
      "showSpinner": true,
      "spinnerColor": "#ffffff"
    },
    "GoogleAuth": {
      "scopes": ["profile", "email"],
      "serverClientId": "214444955038-REPLACE_WITH_WEB_CLIENT_ID.apps.googleusercontent.com",
      "forceCodeForRefreshToken": true
    }
  }
}
